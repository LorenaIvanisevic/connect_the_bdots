      <color rgba="0 0 0.8 1"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 1.0"/>
      </geometry>
    </collision>
  </link-->

<!-- Link 1 - base -->
  <link name="base">
    <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://connect_the_bdots/meshes/SeminarBase.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="">
        <color rgba="0.054901960784313725 0.054901960784313725 0.06274509803921569 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://connect_the_bdots/meshes/SeminarBase.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

  </link>

  <!-- Joint base -->
  <joint name="base_to_link_90" type="revolute">
    <parent link="base"/>
    <child link="link_90"/>
    <origin xyz="0.0 0.0 0.04" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit velocity="10.0" effort="1" lower="0" upper="2.82743338823" />
  </joint>



  <!-- Link 2 - link 90-->
  <link name="link_90">
     <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://connect_the_bdots/meshes/SeminarLink90.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="">
        <color rgba="0.054901960784313725 0.054901960784313725 0.06274509803921569 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://connect_the_bdots/meshes/SeminarLink90.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

 <!-- Joint base -> xl -->
  <joint name="link_90_to_xl" type="fixed">
    <parent link="link_90"/>
    <child link="motor1"/>
    <origin xyz="0.0 0.0 0.030" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit velocity="10.0" effort="1" lower="-2.82743338823" upper="2.82743338823" />
  </joint>

  <!-- Motor 1-->
  <link name="motor1">
     <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0.01"/>
      <geometry>
        <mesh filename="package://connect_the_bdots/meshes/SeminarXL-430_new.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="">
        <color rgba="0.054901960784313725 0.054901960784313725 0.06274509803921569 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="1.57075 0 0" xyz="0 0 0.01"/>
      <geometry>
        <mesh filename="package://connect_the_bdots/meshes/SeminarXL-430_new.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>


  <!-- Joint -->
  <joint name="xl_to_link_RR" type="revolute">
    <parent link="motor1"/>
    <child link="link_RR"/>
    <origin xyz="0.0 0.001 0.005" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit velocity="10.0" effort="1" lower="-2.82743338823" upper="2.82743338823" />
  </joint>

  <!-- Link 3 - link RR-->
  <link name="link_RR">
       <visual>
      <origin rpy="1.57075 0 1.57075" xyz="0.0 0.017 0.005 "/>
      <geometry>
        <mesh filename="package://connect_the_bdots/meshes/SeminarLinkRR.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="">
        <color rgba="0.054901960784313725 0.054901960784313725 0.06274509803921569 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="1.57075 0 0" xyz="0.0 0.017 0.005"/>
      <geometry>
        <mesh filename="package://connect_the_bdots/meshes/SeminarLinkRR.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

   <!-- Joint -->
  <joint name="link_RR_to_xl" type="revolute">
    <parent link="link_RR"/>
    <child link="motor2"/>
    <origin xyz="-0.18 0.0 0.0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit velocity="10.0" effort="1" lower="-2.82743338823" upper="2.82743338823" />
  </joint>

    <!-- Motor 2-->
  <link name="motor2">
     <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0.0"/>
      <geometry>
        <mesh filename="package://connect_the_bdots/meshes/SeminarXL-430_new.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="">
        <color rgba="0.054901960784313725 0.054901960784313725 0.06274509803921569 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="1.57075 0 1.57075" xyz="0 0 0.01"/>
      <geometry>
        <mesh filename="package://connect_the_bdots/meshes/SeminarXL-430_new.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

